for i in range(2): #rows
    for j in range(3): #columns
        print("*", end = "")
    print()
    
print()   
for i in range(2): #rows
    for j in range(1,6): #columns
        print(j, end = "")
    print()
  
print()   
for i in range(5): #rows
    for j in range(i): #columns
        print("*", end = " ")
    print()
